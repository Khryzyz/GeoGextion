<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imvGextionLogo"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:contentDescription="@string/descripcion_logo_gextion"
        android:src="@drawable/logo_gextion" />

    <Button
        android:id="@+id/btnGextionLogout"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imvGextionLogo"
        android:paddingEnd="10dp"
        android:paddingStart="10dp"
        android:text="@string/text_button_logout" />

    <TextView
        android:id="@+id/txvGeoGextionLog"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/btnGextionLogout"
        android:layout_marginBottom="20dp"
        android:background="@color/colorBlack"
        android:paddingEnd="10dp"
        android:paddingStart="10dp"
        android:scrollbars="vertical"
        android:textColor="@color/colorWhite" />


</RelativeLayout>